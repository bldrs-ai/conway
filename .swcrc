{
  "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "decorators": false,
        "dynamicImport": false
      },
      "target": "es2021",
      "paths": {
        "@src/*": ["./src/*"]
      },
      "baseUrl": "./src"
    },
    "module": {
      "type": "commonjs",
      "strict":  false,
      "strictMode": true,
      "lazy": false,
      "noInterop": false
    }
  }
  