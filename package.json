{
  "name": "bldrs-conway",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build-conway_geom": "run-script-os",
    "build-conway_geom:win32": "cd .\\dependencies\\conway-geom\\ && call .\\build_win.bat release && (if not exist Dist (mkdir Dist)) && xcopy .\\bin\\release\\* Dist\\ /Y/r && copy /Y ConwayGeomWasm.d.ts Dist\\ && (if not exist ..\\..\\compiled\\dependencies\\conway-geom\\Dist (mkdir ..\\..\\compiled\\dependencies\\conway-geom\\Dist)) && xcopy .\\bin\\release\\* ..\\..\\compiled\\dependencies\\conway-geom\\Dist\\ /Y/r",
    "build-conway_geom:darwin": "cd ./dependencies/conway-geom/ && ./build_osx.sh release wasm && mkdir -p Dist && cp ./bin/release/* Dist/ && cp ConwayGeomWasm.d.ts Dist/ && mkdir -p ../../compiled/dependencies/conway-geom/Dist && cp ./bin/release/* ../../compiled/dependencies/conway-geom/Dist",
    "build-clean-conway_geom": "run-script-os",
    "build-clean-conway_geom:win32": "cd ./dependencies/conway-geom/ && build_win.bat clean",
    "build-clean-conway_geom:darwin": "cd ./dependencies/conway-geom/ && ./build_osx.sh clean",
    "build-test-conway_geom": "run-script-os",
    "build-test-conway_geom:win32": "cd ./dependencies/conway-geom/ && build_win.bat test",
    "build-test-conway_geom:darwin": "cd ./dependencies/conway-geom/ && ./build_osx.sh test",
    "build-test-watch": "yarn build-incremental && concurrently \"yarn build-watch\" \"yarn test-watch\"",
    "build-watch": "tsc --build --watch",
    "build-incremental": "tsc --build",
    "build-rebuild": "yarn build-clean && yarn code-gen && yarn build-incremental",
    "build-clean": "tsc --build --clean",
    "test-watch": "jest --watchAll  --notify",
    "test": "yarn node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "yarn eslint src",
    "precommit": "yarn build-rebuild && yarn lint && yarn test",
    "dist": "tsc --project tsconfig.dist.json",
    "code-gen": "cd ./external/IFC-gen && make \"OUTDIR=../../src/ifc/ifc4_gen\" && cd ../..",
    "submodule-update": "git submodule update --init --recursive"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.4.3",
    "concurrently": "^7.5.0",
    "eslint": "^8.27.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.3.1",
    "node-notifier": "^10.0.1",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "smart-buffer": "^4.2.0",
    "yargs": "^17.7.1"
  }
}
