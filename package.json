{
  "name": "bldrs-conway",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-incremental-watch-test": "yarn build-incremental && concurrently \"yarn test-watch\"",
    "build-transpile-watch-test": "yarn build-transpile && concurrently \"yarn build-transpile-watch\" \"yarn test-watch\"",
    "build-transpile-watch": "swc src -w -s -d compiled/src",
    "build-transpile": "swc src -s -d compiled/src",
    "build-incremental": "tsc --build",
    "build-rebuild": "yarn build-clean && yarn build-incremental",
    "transpile-clean": "yarn rimraf ./compiled/*.js && yarn rimraf ./compiled/*.js.map && yarn rimraf ./compiled/core/*.js && yarn rimraf ./compiled/core/*.js.map && yarn rimraf ./compiled/gen/*.js && yarn rimraf ./compiled/gen/*.js.map",
    "build-clean": "yarn transpile-clean && tsc --build --clean",
    "test-watch": "jest --watchAll  --notify",
    "test": "jest",
    "lint": "yarn eslint src",
    "precommit": "yarn build-rebuild && yarn lint && yarn test",
    "dist": "tsc --project tsconfig.dist.json && swc src -s -d dist",
    "code-gen": "cd ./external/IFC-gen && ./make \"OUTDIR=../../src/gen/ifc\" && cd ../..",
    "submodule-update": "git submodule update --init --recursive"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.16",
    "@swc/jest": "^0.2.23",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "concurrently": "^7.5.0",
    "eslint": "^8.27.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.3.1",
    "node-notifier": "^10.0.1",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
