{
  "name": "bldrs-conway",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build-conway_geom": "run-script-os",
    "build-conway_geom:win32": "cd ./dependencies/conway-geom/ && build_win.bat release && copy .\\bin\\release ..\\..\\compiled\\dependencies\\conway-geom",
    "build-conway_geom:darwin": "cd ./dependencies/conway-geom/ && ./build_osx.sh release && cp ./bin/release/* ../../compiled/dependencies/conway-geom && cp ./bin/release/* ./",
    "build-test-watch": "yarn build-incremental && concurrently \"yarn build-watch\" \"yarn test-watch\"",
    "build-watch": "tsc --build --watch",
    "build-incremental": "tsc --build",
    "build-rebuild": "yarn build-clean && yarn code-gen && yarn build-incremental",
    "build-clean": "tsc --build --clean",
    "test-watch": "jest --watchAll  --notify",
    "test-geom-extraction": "yarn node --experimental-vm-modules node_modules/jest/bin/jest.js --watchAll --no-verbose --notify --testNamePattern ifc_step_model.test.js",
    "test": "yarn node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "yarn eslint src",
    "precommit": "yarn build-rebuild && yarn lint && yarn test",
    "dist": "tsc --project tsconfig.dist.json",
    "code-gen": "cd ./external/IFC-gen && make \"OUTDIR=../../src/gen/ifc\" && cd ../..",
    "submodule-update": "git submodule update --init --recursive"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.4.3",
    "concurrently": "^7.5.0",
    "eslint": "^8.27.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.3.1",
    "node-notifier": "^10.0.1",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "smart-buffer": "^4.2.0",
    "yargs": "^17.7.1"
  }
}
